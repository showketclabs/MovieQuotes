<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/rv"
android:background="#000000"
android:layout_width="720px"
android:layout_height="1184px" >
<RelativeLayout 
     android:id="@+id/mainlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/screen3bg"
    android:orientation="vertical" >
  <RelativeLayout
        android:id="@+id/add"
        android:layout_width="fill_parent"
        android:layout_height="110px"
        android:gravity="center_vertical|center_horizontal|center" >
        
    </RelativeLayout>
<Button
        android:id="@+id/upgrade_ext"
        android:layout_width="220px"
        android:layout_height="90px"
       
        android:layout_marginTop="270px"
        android:layout_marginLeft="50px"
        
        android:background="@drawable/upgradeextra" />
  <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="220px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true" >
        

        
        

            
        

            <Button
                android:id="@+id/btnsearch"
                android:layout_width="170px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="184px"
                android:background="@drawable/btn_search" />
          

            <Button
                android:id="@+id/btnmore"
                android:layout_width="190px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="530px"
                android:background="@drawable/btn_more" />
     

            <Button
                android:id="@+id/btnfav"
                android:layout_width="188px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_alignTop="@+id/btnsearch"
                android:layout_marginLeft="346px"
                android:background="@drawable/btn_press" />
        

            <Button
                android:id="@+id/btnlist"
                android:layout_width="178px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:background="@drawable/btn_list" />
        

            <Button
                android:id="@+id/btnback"
                android:layout_width="273px"
                android:layout_height="112px"
                android:layout_alignParentLeft="true"
             android:layout_marginTop="5px"
                android:background="@drawable/btn_back" />
<Button
            android:id="@+id/btnshake"
            android:layout_width="290px"
            android:layout_height="136px"
            android:layout_centerHorizontal="true"
           
            android:background="@drawable/btn_shake" />
            <Button
                android:id="@+id/btnhlp"
                android:layout_width="273px"
                android:layout_height="112px"
                android:layout_above="@+id/btnmore"
                android:layout_marginTop="5px"
                android:layout_alignParentRight="true"
                android:background="@drawable/btn_help" />
    
        </RelativeLayout>
    

    <RelativeLayout
        android:layout_width="630px"
        android:layout_height="450px"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="460px"
        android:background="@drawable/textbox" >
        

        <Button
            android:id="@+id/button1"
            android:layout_width="55px"
            android:layout_height="55px"
            android:layout_marginLeft="550px"
            android:layout_marginTop="17.5px"
            android:background="@drawable/btn_arrow" />
         

        <Button
            android:id="@+id/button2"
           android:layout_width="55px"
            android:layout_height="55px"
            android:layout_marginLeft="550px"
            android:layout_marginTop="90px"
            android:background="@drawable/btn_arrow" />
          

        <Button
            android:id="@+id/button3"
           android:layout_width="55px"
            android:layout_height="55px"
            android:layout_marginLeft="550px"
            android:layout_marginTop="195px"
            android:background="@drawable/btn_arrow" />
           

        <Button
            android:id="@+id/button4"
          android:layout_width="55px"
            android:layout_height="55px"
            android:layout_marginLeft="550px"
            android:layout_marginTop="295px"
            android:background="@drawable/btn_arrow" />
            

        <Button
            android:id="@+id/button5"
            android:layout_width="55px"
            android:layout_height="55px"
            android:layout_marginLeft="550px"
            android:layout_marginTop="367.5px"
            android:background="@drawable/btn_arrow" />
    
    </RelativeLayout>

</RelativeLayout>
</FrameLayout>



