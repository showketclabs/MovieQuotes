<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/rv"
android:background="#000000"
android:layout_width="720px"
android:layout_height="1184px" >
<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/screen8_bg"
    android:orientation="vertical" >
 <LinearLayout
        android:id="@+id/banner"
        android:layout_width="720px"
        android:layout_height="110px" >
        
    </LinearLayout>
    <RelativeLayout
            android:layout_width="720px"
            android:layout_height="220px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true" >
        

        
        

            
        

            <Button
                android:id="@+id/btnsearch"
                android:layout_width="170px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="184px"
                android:background="@drawable/btn_search" />
          

            <Button
                android:id="@+id/btnmore"
                android:layout_width="190px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="530px"
                android:background="@drawable/btn_more" />
     

            <Button
                android:id="@+id/btnfav"
                android:layout_width="188px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_alignTop="@+id/btnsearch"
                android:layout_marginLeft="346px"
                android:background="@drawable/btn_press" />
        

            <Button
                android:id="@+id/btnlist"
                android:layout_width="178px"
                android:layout_height="104px"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:background="@drawable/btn_list" />
        

            <Button
                android:id="@+id/btnback"
                android:layout_width="273px"
                android:layout_height="112px"
                android:layout_alignParentLeft="true"
             android:layout_marginTop="5px"
                android:background="@drawable/btn_back" />
<Button
            android:id="@+id/btnshake"
            android:layout_width="290px"
            android:layout_height="136px"
            android:layout_centerHorizontal="true"
           
            android:background="@drawable/btn_shake" />
            <Button
                android:id="@+id/btnhlp"
                android:layout_width="273px"
                android:layout_height="112px"
                android:layout_above="@+id/btnmore"
                android:layout_marginTop="5px"
                android:layout_alignParentRight="true"
                android:background="@drawable/btn_help" />
    
        </RelativeLayout>

    <RelativeLayout
        android:layout_width="605px"
        android:layout_height="300px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="84px" >

        <Button
            android:id="@+id/btnfb"
            android:layout_width="250.5px"
            android:layout_height="100px"
            android:layout_marginLeft="13px"
            android:layout_marginTop="57px"
            android:background="@drawable/btn_facebook" />

        <Button
            android:id="@+id/btnemail"
           android:layout_width="250.5px"
            android:layout_height="100px"
            android:layout_marginLeft="13px"
            android:layout_marginTop="190.5px"
            android:background="@drawable/btn_email" />

        <Button
            android:id="@+id/btntw"
            android:layout_width="250.5px"
            android:layout_height="100px"
            android:layout_alignBaseline="@+id/btnfb"
            android:layout_alignBottom="@+id/btnfb"
            android:layout_alignParentRight="true"
            android:background="@drawable/btn_twitter" />

        <Button
            android:id="@+id/btntext"
             android:layout_width="250.5px"
            android:layout_height="100px"
            android:layout_alignBaseline="@+id/btnemail"
            android:layout_alignBottom="@+id/btnemail"
            android:layout_alignLeft="@+id/btntw"
            android:background="@drawable/btn_text" />

    </RelativeLayout>

</RelativeLayout>
</FrameLayout>