<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/rv"
android:background="#000000"
android:layout_width="480px"
android:layout_height="800px" >
<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/screen8_bg"
    android:orientation="vertical" >
<FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="75px"
        android:layout_marginLeft="0px"
        android:layout_marginTop="0px" >
   </FrameLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="150px"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" >
        

        <Button
            android:id="@+id/btnhlp"
            android:layout_width="180px"
            android:layout_height="70.5px"
            android:layout_marginLeft="300px"
            android:layout_marginTop="9px"
            android:background="@drawable/btn_help" />
        

        <Button
            android:id="@+id/btnshake"
            android:layout_width="192px"
            android:layout_height="90px"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1.5px"
            android:background="@drawable/btn_shake" />
        

        <Button
            android:id="@+id/btnsearch"
            android:layout_width="118.5px"
            android:layout_height="72px"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="120px"
            android:background="@drawable/btn_search" />
        

        <Button
            android:id="@+id/btnmore"
            android:layout_width="120px"
            android:layout_height="72px"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="358.5px"
            android:background="@drawable/btn_more" />
        

        <Button
            android:id="@+id/btnfav"
            android:layout_width="118.5px"
            android:layout_height="72px"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="240px"
            android:background="@drawable/btn_press" />
        

        <Button
            android:id="@+id/btnlist"
            android:layout_width="118.5px"
            android:layout_height="72px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="@drawable/btn_list" />
        

        <Button
            android:id="@+id/btnback"
            android:layout_width="180px"
            android:layout_height="70.5px"
            android:layout_alignParentLeft="true"
            android:layout_alignTop="@+id/btnhlp"
            android:background="@drawable/btn_back" />
    
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="405px"
        android:layout_height="225px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54px" >

        <Button
            android:id="@+id/btnfb"
            android:layout_width="187.5px"
            android:layout_height="75px"
            android:layout_marginLeft="3px"
            android:layout_marginTop="57px"
            android:background="@drawable/btn_facebook" />

        <Button
            android:id="@+id/btnemail"
            android:layout_width="187.5px"
            android:layout_height="75px"
            android:layout_marginLeft="3px"
            android:layout_marginTop="142.5px"
            android:background="@drawable/btn_email" />

        <Button
            android:id="@+id/btntw"
            android:layout_width="187.5px"
            android:layout_height="75px"
            android:layout_alignBaseline="@+id/btnfb"
            android:layout_alignBottom="@+id/btnfb"
            android:layout_alignParentRight="true"
            android:background="@drawable/btn_twitter" />

        <Button
            android:id="@+id/btntext"
            android:layout_width="187.5px"
            android:layout_height="75px"
            android:layout_alignBaseline="@+id/btnemail"
            android:layout_alignBottom="@+id/btnemail"
            android:layout_alignLeft="@+id/btntw"
            android:background="@drawable/btn_text" />

    </RelativeLayout>

</RelativeLayout>
</FrameLayout>