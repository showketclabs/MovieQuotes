<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/rv"
android:background="#000000"
android:layout_width="720px"
android:layout_height="1184px" >
<RelativeLayout
    android:id="@+id/mainlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/splashscreen"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="578px"
        android:layout_height="250px"
        android:layout_marginTop="700px"
         android:layout_marginLeft="40px"
        android:layout_marginBottom="0px"
        android:visibility="invisible"
        android:src="@drawable/gazillion" />
      <ImageView
        android:id="@+id/imageView2"
        android:layout_width="420px"
        android:layout_height="wrap_content"
        android:layout_marginTop="845px"
         android:layout_marginLeft="170px"
        android:layout_marginBottom="0px"
        android:visibility="invisible"
        android:paddingTop="70px"
        android:src="@drawable/movie_quotes" />
    

</RelativeLayout></FrameLayout>