<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/rv"
android:background="#000000"
android:layout_width="480px"
android:layout_height="800px" >
<RelativeLayout 
       android:id="@+id/mainlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/screen10"
    android:orientation="vertical" >
    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="75.00px"
        android:layout_marginLeft="0px"
        android:layout_marginTop="0px" >
   </FrameLayout>
    

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="150px"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" >
        

        <Button
            android:id="@+id/btnhlp"
            android:layout_width="180.00px"
            android:layout_height="70.50px"
            android:layout_marginLeft="300.00px"
            android:layout_marginTop="9.00px"
            android:background="@drawable/btn_help" />
        

        <Button
            android:id="@+id/btnback"
            android:layout_width="180.00px"
            android:layout_height="70.50px"
            android:layout_alignParentLeft="true"
            android:layout_alignTop="@+id/btnhlp"
            android:background="@drawable/btn_back" />
        

        <Button
            android:id="@+id/btnshake"
            android:layout_width="192.00px"
            android:layout_height="90.00px"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1.50px"
            android:background="@drawable/btn_shake" />
        

        <Button
            android:id="@+id/btnlist"
            android:layout_width="fill_parent"
            android:layout_height="72.00px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:background="@drawable/btn_purchase" />
    
    </RelativeLayout>
    

    
    

    <Button
        android:id="@+id/button2"
        android:layout_width="187.50px"
        android:layout_height="76.50px"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="37.50px"
        android:layout_marginTop="150px"
        android:background="@drawable/btncapupgrade" />

</RelativeLayout>
</FrameLayout>